    <link rel="shortcut icon" href="/cargopress/html/images/fav.png">

    <script>
        /* Google Map Market Settings */

        var mapMarkers = [
            {
                "id": "1",
                "title": "CargoPress",
                "address": "Changi Airport, Singapore",
                "custompinimage": "images\/pin.png"
            }
        ]
    </script>

</head>
